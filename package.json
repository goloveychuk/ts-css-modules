{
  "name": "tsruntime",
  "version": "1.1.0",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rimraf dist && tsc --pretty",
    "test": "npm run build && karma start",
    "watch": "rimraf dist && tsc --watch --pretty",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "build:test": "rimraf ./tests/build && rimraf ./tests/emit_output && tsc -p ./tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goloveychuk/tsruntime.git"
  },
  "keywords": [
    "typescript",
    "reflect",
    "runtime"
  ],
  "bugs": {
    "url": "https://github.com/goloveychuk/tsruntime/issues"
  },
  "homepage": "https://github.com/goloveychuk/tsruntime",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^2.2.34",
    "@types/node": "^7.0.18",
    "@types/react": "^15.0.24",
    "jasmine": "~2.5.2",
    "karma": "~1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-mocha-reporter": "~2.2.3",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~2.0.3",
    "react": "^15.5.4",
    "rimraf": "^2.6.1",
    "typescript": "^2.3.2",
    "webpack": "2.2.0"
  }
